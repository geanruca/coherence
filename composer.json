{
  "name": "geanruca/laravel-coherence",
  "description": "A Laravel package for AI-based data coherence validation in you Eloquent Models",
  "type": "library",
  "require": {
    "php": "^8.1",
    "prism-php/prism": "^0.67.0",
    "illuminate/support": "^10.0|^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.0",
    "orchestra/testbench": "^8.0 || ^9.0 || ^10.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Geanruca\\LaravelCoherence\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Geanruca\\LaravelCoherence\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Geanruca\\LaravelCoherence\\Providers\\CoherenceServiceProvider"
      ]
    }
  },
  "authors": [
    {
      "name": "Gerardo Ruiz",
      "email": "geanruca@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
